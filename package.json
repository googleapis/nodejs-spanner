    "events-intercept": "^2.0.0",
    "extend": "^3.0.0",
    "generic-pool": "^3.1.4",
    "google-gax": "^0.13.0",
    "google-proto-files": "^0.12.0",
    "is": "^3.1.0",
    "lodash.chunk": "^4.2.0",
    "lodash.flatten": "^4.4.0",
    "lodash.snakecase": "^4.1.1",
    "merge-stream": "^1.0.1",
    "methmeth": "^1.1.0",
    "safe-buffer": "^5.1.1",
    "split-array-stream": "^1.0.0",
    "stream-events": "^1.0.1",
    "string-format-obj": "^1.1.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^2.0.10",
    "async": "^2.5.0",
    "codecov": "^2.3.1",
    "concat-stream": "^1.6.0",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-prettier": "^2.3.1",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^3.5.3",
    "multiline": "^1.0.2",
    "nyc": "^11.2.1",
    "power-assert": "^1.4.4",
    "prettier": "^1.7.4",
    "proxyquire": "^1.7.11",
    "uuid": "^3.0.1"
  }
